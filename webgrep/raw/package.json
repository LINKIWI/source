{
  "name": "webgrep",
  "version": "1.0.0",
  "description": "Livegrep frontend",
  "bin": "dist/server/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run build:client && npm run build:server && npm run build:shared",
    "build:client": "webpack",
    "build:client:watch": "npm run build:client -- --watch",
    "build:server": "babel src/server -d dist/server --copy-files --verbose",
    "build:server:watch": "npm run build:server -- --watch",
    "build:shared": "babel src/shared -d dist/shared --copy-files --verbose",
    "build:shared:watch": "npm run build:shared -- --watch",
    "start:client": "webpack-dev-server",
    "start:client:watch": "npm run start:client -- --watch",
    "start:server": "node -r @babel/register src/server -- --verbosity debug",
    "start:server:watch": "nodemon -r @babel/register src/server -w src/server -- --verbosity debug",
    "release": "pkg --targets node16-linux-x64 --output dist/bin/webgrep .",
    "serve": "node dist/server",
    "dev": "concurrently --kill-others --prefix name --prefix-colors cyan.bold,magenta.bold --names HTTP,JS 'npm run start:server:watch' 'npm run build:client:watch'",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "cover": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://source.internal.kevinlin.info/personal/webgrep.git"
  },
  "author": "Kevin Lin <developer@kevinlin.info>",
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ],
  "pkg": {
    "assets": [
      "dist/client/**/*",
      "dist/server/**/*",
      "dist/shared/**/*"
    ]
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.5",
    "@linkiwi/fonts": "https://lib.kevinlin.info/fonts/9d03f84000c20d139ad2a10a08c477fb0c38f62a/release.tgz",
    "@linkiwi/hoc": "https://lib.kevinlin.info/hoc/5a44275bdf250d97fa5179928a1a93448c843118/release.tgz",
    "@sentry/browser": "^6.17.7",
    "@sentry/node": "^6.17.7",
    "async": "^3.2.3",
    "color": "^4.2.1",
    "express": "^4.17.2",
    "gitlab": "https://lib.kevinlin.info/external/gitbeaker/a798868e7892aa56d2e767db71fc42fa9a0ae74b/release.tgz",
    "google-protobuf": "^3.19.4",
    "humanize": "^0.0.9",
    "ioredis": "^4.28.5",
    "isomorphic-git": "^1.11.2",
    "js-yaml": "^4.1.0",
    "livegrep": "https://lib.kevinlin.info/schemas/external/livegrep/c62b1eae84a21d3d50e760060ba7bf52303a1814/release.tgz",
    "node-statsd": "^0.1.1",
    "prop-types": "^15.8.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-elemental": "https://lib.kevinlin.info/react-elemental/1ad5838ec41b5778c4430c7e760e0e7eadc9c9bf/release.tgz",
    "react-favicon": "^0.0.26",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^13.5.1",
    "recompose": "^0.30.0",
    "reconnectingwebsocket": "^1.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.4.1",
    "snappyjs": "^0.6.0",
    "supercharged": "https://lib.kevinlin.info/supercharged/7a0c4d26d55651d7a759b85c1a863ff1052749da/release.tgz",
    "winston": "^3.6.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.0",
    "@linkiwi/eslint-config": "https://lib.kevinlin.info/eslint-config/cbf6e408cd5706df66e770d2ca7960e20283b6a9/release.tgz",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-elemental": "^1.2.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^8.9.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-react": "^7.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "nodemon": "^2.0.15",
    "pkg": "^5.5.2",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.1",
    "react-test-renderer": "^16.14.0",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
