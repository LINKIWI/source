{
  "name": "webgrep",
  "version": "1.0.0",
  "description": "Livegrep frontend",
  "bin": "dist/server/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run build:client && npm run build:server && npm run build:shared",
    "build:client": "webpack",
    "build:client:watch": "npm run build:client -- --watch",
    "build:server": "babel src/server -d dist/server --copy-files --verbose",
    "build:server:watch": "npm run build:server -- --watch",
    "build:shared": "babel src/shared -d dist/shared --copy-files --verbose",
    "build:shared:watch": "npm run build:shared -- --watch",
    "build:schemas": "pbjs --target static-module --wrap es6 --lint eslint-disable src/shared/schemas/*.proto > src/shared/schemas/index.js",
    "start:client": "webpack-dev-server",
    "start:client:watch": "npm run start:client -- --watch",
    "start:server": "node -r @babel/register src/server",
    "start:server:watch": "nodemon -r @babel/register src/server -w src/server",
    "release": "pkg --targets node14-linux-x64 --output dist/bin/webgrep .",
    "serve": "node dist/server",
    "dev": "concurrently --kill-others --prefix name --prefix-colors cyan.bold,magenta.bold --names HTTP,JS 'npm run start:server:watch' 'npm run build:client:watch'",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "cover": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LINKIWI/webgrep.git"
  },
  "author": "Kevin Lin <developer@kevinlin.info>",
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ],
  "pkg": {
    "assets": [
      "dist/client/**/*",
      "dist/shared/schemas/**/*"
    ]
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.1.5",
    "@grpc/proto-loader": "^0.5.5",
    "@linkiwi/hoc": "^0.0.2",
    "@sentry/browser": "^5.21.1",
    "@sentry/node": "^5.21.1",
    "async": "^3.2.0",
    "color": "^3.1.2",
    "express": "^4.17.1",
    "gitlab": "^14.2.2",
    "humanize": "^0.0.9",
    "ioredis": "^4.17.3",
    "isomorphic-git": "^1.7.4",
    "js-yaml": "^3.14.0",
    "node-statsd": "^0.1.1",
    "prop-types": "^15.7.2",
    "protobufjs": "^6.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-elemental": "https://react-elemental.static.kevinlin.info/releases/fad631c1f5b29bc8421818562e09db21eb9fc9a0/package.tgz",
    "react-elemental-fonts": "^0.0.2",
    "react-favicon": "^0.0.18",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^13.5.1",
    "recompose": "^0.30.0",
    "reconnectingwebsocket": "^1.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "snappyjs": "^0.6.0",
    "supercharged": "https://supercharged.static.kevinlin.info/releases/8e191303a3bc18a873e47ae6a89c389b7a698d49/package.tgz",
    "winston": "^3.3.3",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@linkiwi/eslint-config": "^2.1.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-elemental": "^1.2.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "base64-inline-loader": "^1.1.1",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.7.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-react": "^7.20.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.1",
    "nodemon": "^2.0.4",
    "pkg": "^4.4.9",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "terser-webpack-plugin": "^4.1.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
